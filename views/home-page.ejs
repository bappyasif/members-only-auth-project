<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head.ejs") %>
<body>    
    <div class="container">
        <%- include("./partials/navs.ejs") %>
        
        <h1><%= title %></h1>
        
        <% messages?.forEach(msg => { %>
            <div class="msg-wrapper">
                <div class="left">
                    <span><i class="fa-solid fa-user-astronaut"></i></span>
                </div>
                
                <%- include("./partials/msg-middle-part", {msg: msg}) %>

                <%- include("./partials/msg-right-part.ejs", {msg: msg}) %>
                
                <%- include("./partials/msg-admin-view.ejs", {msg: msg}) %>
            </div>
        <% }) %>
    </div>
</body>
</html>